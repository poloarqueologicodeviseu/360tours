function toggleFloor() {
  var x = document.getElementById("floorBar");
  if (x.style.width === "0%") {
    x.style.width = "30%";
    x.style.opacity = "1.0";
  } else {
    x.style.opacity = "0.0";
    setTimeout(closewidth, 500);
  }
}

function closewidth() {
  var x = document.getElementById("floorBar");
  x.style.width = "0%";
}
</script>



